{
  "name": "@textile/desktop",
  "version": "0.1.6",
  "description": "Textile Desktop",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --ext .mjs,.js,.jsx,.ts,.tsx src && echo 'Lint complete.'",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "rebuild-deps": "electron-builder install-app-deps",
    "dist": "npm run compile && electron-builder -c.asarUnpack=**/node_modules/@textile/go-textile-dep/**/*",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 3.0.0",
    "yarn": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-textile-wallet.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-textile-wallet/issues"
  },
  "keywords": [
    "textile",
    "crypto",
    "wallet",
    "IPFS",
    "p2p"
  ],
  "author": "textile.io",
  "license": "MIT",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@textile/go-daemon": "^0.1.3",
    "@textile/go-textile-dep": "^0.6.5",
    "@textile/js-http-client": "^1.0.1",
    "adm-zip": "^0.4.13",
    "electron": "^6.0.1",
    "keytar": "^5.0.0-beta.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-sync": "^2.1.2",
    "moment": "^2.24.0",
    "query-string": "^6.8.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-moment": "^0.9.2",
    "react-reveal": "^1.2.2",
    "react-semantic-toasts": "^0.6.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.3",
    "source-map-support": "^0.5.13",
    "url-parse": "^1.4.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@types/bip39": "^2.4.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^3.4.5",
    "@types/zxcvbn": "^4.4.0",
